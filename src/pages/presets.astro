---
import Layout from '@layouts/Layout.astro'

import { presets } from '@/data/presets'

const presetKeys = Object.keys(presets)
---

<Layout>
	<div class='menu'>
		<ul>
			{
				presetKeys.map((key) => (
					<li>
						<a href={`preset/${key}`}>{key}</a>
					</li>
				))
			}
		</ul>
	</div>
</Layout>

<style>
	.menu {
		height: 100%;
		display: grid;
		place-content: center;
	}
</style>
